<% unless comment.archived? %>
  <p>
    <strong>Commenter:</strong>
    <%= comment.commenter %>
  </p>

  <p>
    <strong>Comment:</strong>
    <%= comment.body %>
  </p>
  <td><%= button_to 'Delete', article_comment_path(comment.article,comment), method: :delete, data: {confirm: "Are you sure?"} %></td>
<% end %>
